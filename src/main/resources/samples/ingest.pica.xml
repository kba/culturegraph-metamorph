<?xml version="1.0" encoding="UTF-8"?>
<metamorph xmlns="http://www.culturegraph.org/metamorph"
	entityMarker="." version="1">

	<meta>
		<name>Umsetzung Title</name>
	</meta>

	<rules>
		<data source="001B.0" name="xbib:lastModifiedByInstitution" />

		<data source="002@.0" name="cg:typeOfWork">
			<substring start="1" end="2" />
			<lookup in="workList" />
		</data>

		<data source="002@.0" name="cg:typeOfConWork">
			<substring start="1" end="2" />
			<lookup in="workListContinuousWorks" />
		</data>

		<!-- <data source="002@.0" name="cg:statusWork"> <substring start="2" end="3"/> 
			<lookup in="statusList" /> </data> -->

		<data source="002@.0" name="dcterms:format">
			<substring start="0" end="1" />
			<lookup in="material" />
		</data>

		<data source="003@.0" name="cg:sourceRecordId" />

		<data source="004A.0" name="xbib:identifierAuthorityISBN">
			<isbn to="isbn13" />
		</data>

		<data source="004F.0" name="xbib:identifierAuthorityISMN" />

		<data source="004K.0" name="xbib:identifierAuthorityEAN" />

		<data source="004M.0" name="xbib:identifierAuthorityISRN" />

		<data source="004V.0" name="xbib:uniformResourceDoi" />

		<data source="004U.0" name="xbib:uniformResourceName" />

		<data source="004R.0" name="xbib:uniformResourceHandle" />

		<data source="005A.0" name="xbib:identifierAuthorityISSN" />

		<data source="006Y.0" name="xbib:identifierPatent" />

		<group name="xbib:identifierAuthorityDNB">
			<data source="006G.0" />
			<data source="003@.0" />
		</group>

		<data source="006Z.0" name="xbib:identifierAuthorityZDB" />

		<data source="006W.0" name="xbib:identifierVD18" />

		<data source="006V.0" name="xbib:identifierVD16" />

		<data source="006R.0" name="xbib:identifierVD17" />

		<combine name="xbib:identifierAuthorityEKI"
			value="${prefix}${id}">
			<data source="007G.a" name="prefix" />
			<data source="007G.0" name="id" />
		</combine>

		<data source="007Ha.0" name="xbib:identifierRegioKOBV" />
		<data source="007Hd.0" name="xbib:identifierRegioHBZ" />
		<data source="007He.0" name="xbib:identifierRegioHEBIS" />
		<data source="007Hf.0" name="xbib:identifierRegioBSZ" />
		<data source="007Hg.0" name="xbib:identifierRegioBVB" />
		<data source="007Hh.0" name="xbib:identifierRegioGBV" />

		<data source="007Ib.0" name="xbib:identifierAuthorityBNB" />
		<data source="007Ic.0" name="xbib:identifierAuthorityCasaliniLibri" />
		<data source="007Ie.0" name="xbib:identifierAuthorityEKZ" />
		<data source="007Ig.0" name="xbib:identifierAuthorityBNF" />
		<data source="007Il.0" name="xbib:identifierAuthorityLOC" />
		<data source="007Io.0" name="xbib:identifierAuthorityOCLC" />

		<data source="007E.0" name="xbib:identifierUniversityScript" />

		<data source="007P.0" name="xbib:fingerprint" />

		<data source="010@.a" name="dcterms:language" />

		<data source="010@.c" name="xbib:originLanguage" />

		<group name="dcterms:issued">
			<data source="011@.a" />
			<data source="011@.n" />
		</group>

		<data source="013H.0" name="dcterms:format">
		</data>

		<data source="019@a" name="cg:countryCodeIso" />

		<group name="@dcterms:title">
			<data source="021A.a" />
			<data source="021B.a" />
		</group>

		<data source="@dcterms:title" name="dcterms:title">
			<normalize-utf8 />
		</data>

		<group name="@cg:additionToTitle">
			<data source="021A.d" />
			<data source="021B.d" />
		</group>

		<data source="@cg:additionToTitle" name="cg:additionToTitle">
			<normalize-utf8 />
		</data>


		<data source="021A.9" name="cg:broaderWorkId" />

		<data source="021B.l" name="dcterms:volume" />

		<data source="032@.a" name="cg:edition" />

		<group name="xbib:creatorDNBGND">
			<data source="028A.9" />
			<data source="028A.7" />
		</group>

		<combine name="dcterms:creator"
			value="${surname}${forename}${prefix}${personalname}${qualifier}"
			flushWith="028A">
			<data source="028A.a" name="surname" />
			<data source="028A.d" name="forename">
				<compose prefix=", " />
			</data>
			<data source="028A.c" name="prefix">
				<compose prefix=" " />
			</data>
			<data source="028A.l" name="qualifier">
				<compose prefix=" &lt;" postfix="&gt;" />
			</data>
			<data source="028A.5" name="personalname" />
		</combine>

		<group name="rdaGr2:identifierForTheCorporateBody">
			<data source="029A.0" />
		</group>

		<group name="cg:identifierForTheSecondaryCorporateBody">
			<data source="029F.0" />
		</group>

		<group name="gnd:preferredNameForTheCorporateBody">
			<data source="029A.a" />
			<data source="029A.b" />
		</group>

		<data source="033A.p" name="xbib:publicationPlace" />
		<data source="033A.n" name="xbib:publisherName" />

		<data source="034D.a" name="dcterms:extends" />

	</rules>
	<maps>
		<map name="workList">
			<entry name="a" value="independentWork" />
			<entry name="c" value="severalVolumesWorkLimited" />
			<entry name="d" value="journalSeriesNewspaper" />
			<entry name="E" value="severalVolWorkLtdPartOfBigger " />
			<entry name="F" value="singleVolumeWorkPartOfBigger" />
		</map>

		<map name="workListContinuousWorks">
			<entry name="b" value="journalSeriesNewspaper" />
			<entry name="f" value="dependentConWork" />
			<entry name="l" value="journalSeriesNewspaper" />
			<entry name="p" value="journalSeriesNewspaper" />
		</map>

		<map name="material">
			<entry name="A" value="print" />
			<entry name="B" value="audiovisual" />
			<entry name="O" value="online" />
		</map>
		<!-- <map name="statusList"> <entry name="a" value="provisorischerDatensatz" 
			/> <entry name="c" value="DatensatzNeuerscheinungsdienst" /> <entry name="d" 
			value="HssReiheH" /> <entry name="f" value="Fremddaten" /> <entry name="g" 
			value="konvDatensHss " /> <entry name="h" value="konvZeitschriftenDaten" 
			/> <entry name="l" value="konvDaten" /> <entry name="r" value="konvDaten" 
			/> <entry name="s" value="konvDaten" /> <entry name="n" value="altDaten" 
			/> <entry name="o" value="altDaten" /> <entry name="t" value="altDaten" /> 
			</map> -->
	</maps>
</metamorph>
